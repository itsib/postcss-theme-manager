{
  "name": "postcss-theme-manager",
  "version": "0.0.14",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "description": "PostCSS plugin for generate multiple themes CSS files",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "theme"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -P tsconfig.build.json && vite build --mode=production",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "author": "Sergey Krainov <itsib.su@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsib/postcss-theme-manager.git"
  },
  "bugs": {
    "url": "https://github.com/itsib/postcss-theme-manager/issues"
  },
  "homepage": "https://github.com/itsib/postcss-theme-manager",
  "dependencies": {
    "caniuse-api": "^3.0.0",
    "cssesc": "^3.0.0",
    "deepmerge": "^4.3.1",
    "dlv": "^1.1.3",
    "dset": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.4",
    "@types/caniuse-api": "^3.0.6",
    "@types/cssesc": "^3.0.2",
    "@types/debug": "^4.1.12",
    "@types/dlv": "^1.1.4",
    "@types/flat": "^5.0.5",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "browserslist": "^4.23.3",
    "caniuse-api": "^3.0.0",
    "debug": "^4.3.7",
    "flat": "^6.0.1",
    "postcss": "^8.4.47",
    "postcss-nested": "^6.2.0",
    "rollup-plugin-node-externals": "^7.1.3",
    "tapable": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.6"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
